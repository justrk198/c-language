#include<stdio.h>
#include<math.h>
void main()
{
    int a,b,c,discr,root1,root2;
    printf("enter the eqation:");
    scanf("%d%d%d",&a,&b,&c);
    discr=b*b-4*a*c;
    if(discr>0)
  root1=(-b+sqrt(discr))/(2*a);
   root2=(-b-sqrt(discr))/(2*a);
   printf("root1=%d",root1);
   printf("\n");
   printf("root2=%d",root2);
}
